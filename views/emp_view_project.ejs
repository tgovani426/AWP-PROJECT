<!DOCTYPE html>
<html lang="en" class="">

<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>MiniSAP | View Projects</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">
  <!-- Tailwind is included -->
  <link rel="stylesheet" href="/css/admin.css?v=1652870200386">
  <!-- Font Awesome -->
  <link rel="shortcut icon" href="img/favicon.svg" type="image/x-icon">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.2.0/css/all.min.css">
</head>

<body>
  <div id="app">
    <aside class="aside is-placed-left is-expanded">
      <div class="aside-tools">
        <div>
          <span class="icon"><i class="fa-solid fa-gem"></i></span>
          <b class="font-black">MiniSAP</b>
        </div>
      </div>
      <div class="menu is-menu-main">
        <p class="menu-label">General</p>
        <ul class="menu-list">
          <li class="active">
            <a href="/emp/123/dashboard">
              <span class="icon"><i class="fa-solid fa-gauge"></i></span>
              <span class="menu-item-label">Dashboard</span>
            </a>
          </li>
        </ul>
        <p class="menu-label">PROJECT MANAGEMENT</p>
        <ul class="menu-list">
          
          <li class="--set-active-forms-html">
            <a href="/emp/viewproject">
              <span class="icon"><i class="fa-solid fa-list-check"></i></span>
              <span class="menu-item-label">View Projects</span>
            </a>
          </li>
        </ul>
        
        <p class="menu-label">Actions</p>
        <ul class="menu-list">
          <li class="active">
            <a href="/auth/signin">
              <span class="icon"><i class="fa-solid fa-arrow-right-from-bracket"></i></span>
              <span class="menu-item-label">Log Out</span>
            </a>
          </li>
        </ul>
      </div>
    </aside>

    <section class="is-title-bar">
      <div class="flex flex-col md:flex-row items-center justify-between space-y-6 md:space-y-0">
        <ul>
          <li>Projects</li>
          <li>View</li>
        </ul>
      </div>
    </section>

    <section class="section main-section">
      <div class="card has-table">
        <div class="card-content">
          <table class="table">
            <thead>
              <tr>
                <th>Project Name</th>
                <th>Client Manager</th>
                <th>Project Company Name</th>
                <th>Company Email</th>
                <th>Project ID</th>
                <th>Project Manager</th>
                <th>Department</th>
                <th>Budget</th>
                <th>Deadline</th>
              </tr>
            </thead>
            <tbody>
              <% projectsList.forEach(project=> {%>
                <tr>
                  <td data-label="ProjID">
                    <%= project.projname %>
                  </td>
                  <td data-label="ProjName">
                    <%= project.clientmgr %>
                  </td>
                  <td data-label="ProjClientt">
                    <%= project.clientcompanynm %>
                  </td>
                  <td data-label="ProjDept">
                    <%= project.email %>
                  </td>
                  <td data-label="ProjManager">
                    <%= project.projectid %>
                  </td>
                  <td data-label="ProjManager">
                    <%= project.projectmgr %>
                  </td>
                  <td data-label="ProjManager">
                    <%= project.department %>
                  </td>
                  <td data-label="ProjManager">
                    <%= project.budget %>
                  </td>
                  <td data-label="ProjDeadline">
                    <small class="text-gray-500" title="Oct 25, 2021">
                      <%= project.DOB %>
                    </small>
                  </td>
                </tr>
                <%})%>
            </tbody>
          </table>
        </div>
      </div>
    </section>

  </div>
</body>
</td>

</html>